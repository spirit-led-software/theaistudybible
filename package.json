{
  "name": "revelationsai",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently  --names \"SST,CodeGen\" -c \"bgYellow,bgBlue\" \"sst dev\" \"graphql-codegen --watch './graphql/schema.graphql' --watch './codegen.ts'\"",
    "build": "concurrently --names \"SST,CodeGen\" -c \"bgYellow,bgBlue\" \"sst build --stage\" \"graphql-codegen\"",
    "deploy": "sst deploy --stage",
    "remove": "sst remove --stage",
    "console": "sst console",
    "typecheck": "tsc --noEmit",
    "typecheck:all": "pnpm run typecheck && pnpm run -r --parallel typecheck",
    "lint": "eslint .",
    "format": "prettier --write .",
    "codegen": "graphql-codegen --config codegen.ts",
    "codegen:watch": "graphql-codegen --watch './graphql/schema.graphql' --watch './codegen.ts'",
    "update": "concurrently --names \"Dependency Update, Chromium Layer Update, Argon Layer Update\" -c \"bgYellow,bgBlue,bgPink\" \"pnpm update -rLf && pnpm sst update && pnpm install -rf\" \"cd lambda-layers/chromium && ./build-layer.sh\" \"cd lambda-layers/argon2 && ./build-layer.sh\""
  },
  "devDependencies": {
    "@0no-co/graphqlsp": "^1.9.1",
    "@eslint/eslintrc": "^3.0.2",
    "@eslint/js": "^8.57.0",
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/client-preset": "^4.2.5",
    "@graphql-codegen/introspection": "4.0.3",
    "@graphql-codegen/typescript": "4.0.6",
    "@graphql-codegen/typescript-resolvers": "4.0.6",
    "@parcel/watcher": "^2.4.1",
    "@tsconfig/node20": "^20.1.4",
    "@types/cfn-response": "^1.0.8",
    "@types/in-app-purchase": "^1.11.4",
    "@types/node": "^20.12.4",
    "@types/pg": "^8.11.4",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "aws-cdk-lib": "2.132.1",
    "concurrently": "^8.2.2",
    "constructs": "10.3.0",
    "drizzle-kit": "0.20.14",
    "eslint": "^8.57.0",
    "eslint-plugin-svelte": "^2.35.1",
    "graphql": "^16.8.1",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.2",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "sst": "2.41.4",
    "typescript": "^5.4.4",
    "typescript-eslint": "^7.5.0"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.549.0",
    "@aws-sdk/client-s3": "^3.549.0",
    "@aws-sdk/client-sqs": "^3.549.0",
    "@aws-sdk/credential-providers": "^3.549.0",
    "@aws-sdk/s3-request-presigner": "^3.549.0",
    "@langchain/anthropic": "^0.1.10",
    "@langchain/community": "^0.0.44",
    "@langchain/openai": "^0.0.26",
    "@neondatabase/serverless": "^0.9.0",
    "@upstash/vector": "^1.0.5",
    "drizzle-orm": "^0.30.7",
    "drizzle-zod": "^0.5.1",
    "gql.tada": "^1.4.3",
    "langchain": "^0.1.31",
    "neon-sdk": "^1.18.0",
    "pg": "^8.11.5",
    "pgvector": "^0.1.8",
    "svelte2tsx": "^0.7.6",
    "youtube-transcript": "^1.1.0",
    "youtubei.js": "^9.2.0",
    "zod": "^3.22.4"
  },
  "workspaces": [
    "packages/*"
  ]
}