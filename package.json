{
  "name": "revelationsai",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently  --names \"SST,CodeGen\" -c \"bgYellow,bgBlue,bgMagenta\" \"sst dev\" \"graphql-codegen --watch './graphql/schema.graphql' --watch './codegen.ts'\"",
    "build": "sst build --stage",
    "build:all": "sst build && pnpm run -r --parallel build",
    "deploy": "sst deploy --stage",
    "remove": "sst remove --stage",
    "console": "sst console",
    "typecheck": "tsc --noEmit",
    "typecheck:all": "pnpm run typecheck && pnpm run -r --parallel typecheck",
    "lint": "eslint .",
    "lint:all": "pnpm run lint && pnpm run -r --parallel lint",
    "format": "prettier --write .",
    "format:all": "pnpm run format && pnpm run -r --parallel format",
    "codegen": "graphql-codegen --config codegen.ts",
    "codegen:watch": "graphql-codegen --watch './graphql/schema.graphql' --watch './codegen.ts'"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/introspection": "4.0.3",
    "@graphql-codegen/typescript": "4.0.6",
    "@graphql-codegen/typescript-resolvers": "4.0.6",
    "@parcel/watcher": "^2.4.1",
    "@tsconfig/node20": "^20.1.2",
    "@types/cfn-response": "^1.0.8",
    "@types/in-app-purchase": "^1.11.4",
    "@types/node": "^20.11.19",
    "@types/pg": "^8.11.0",
    "aws-cdk-lib": "2.124.0",
    "concurrently": "^8.2.2",
    "constructs": "10.3.0",
    "drizzle-kit": "0.20.14",
    "eslint": "^8.56.0",
    "prettier": "^3.2.5",
    "sst": "2.40.3",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.515.0",
    "@aws-sdk/client-s3": "^3.515.0",
    "@aws-sdk/client-sqs": "^3.515.0",
    "@aws-sdk/credential-providers": "^3.515.0",
    "@aws-sdk/s3-request-presigner": "^3.515.0",
    "@langchain/community": "^0.0.31",
    "@langchain/google-genai": "^0.0.10",
    "@langchain/openai": "^0.0.14",
    "@neondatabase/serverless": "^0.8.1",
    "drizzle-orm": "^0.29.3",
    "drizzle-zod": "^0.5.1",
    "langchain": "^0.1.20",
    "neon-sdk": "^1.17.0",
    "pg": "^8.11.3",
    "pgvector": "^0.1.8",
    "youtube-transcript": "^1.0.6",
    "youtubei.js": "^9.0.2",
    "zod": "^3.22.4"
  },
  "workspaces": [
    "packages/*"
  ]
}
