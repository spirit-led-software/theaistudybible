{
  "name": "theaistudybible",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "sst dev",
    "deploy": "sst deploy",
    "remove": "sst remove",
    "refresh": "sst refresh",
    "db:generate": "sst shell -- drizzle-kit generate",
    "db:studio": "sst shell -- drizzle-kit studio",
    "scripts": "cd apps/scripts && sst shell -- tsx src/index.ts",
    "format": "pnpm run -r --parallel format",
    "lint": "pnpm run -r --parallel lint",
    "lint:fix": "pnpm run -r --parallel lint:fix",
    "typecheck": "tsc --noEmit",
    "typecheck:all": "pnpm run typecheck && pnpm run -r --parallel typecheck",
    "update": "concurrently --names \"Dependency Update,Chromium Layer Update,LLRT Layer Update\" -c \"bgYellow,bgBlue,bgRed\" \"pnpm update -rLf && pnpm sst upgrade && pnpm install -rf\" \"cd lambda-layers/chromium && ./build-layer.sh\" \"cd lambda-layers/llrt && ./build-layer.sh\" "
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.138",
    "@types/node": "^20.14.1",
    "@types/pg": "^8.11.6",
    "concurrently": "^8.2.2",
    "drizzle-kit": "0.22.2",
    "graphql": "^16.8.1",
    "typescript": "^5.4.5",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.588.0",
    "@aws-sdk/client-cloudfront": "^3.588.0",
    "@aws-sdk/client-s3": "^3.588.0",
    "@aws-sdk/client-sqs": "^3.588.0",
    "@aws-sdk/credential-providers": "^3.588.0",
    "@aws-sdk/middleware-retry": "^3.374.0",
    "@aws-sdk/s3-request-presigner": "^3.588.0",
    "@neondatabase/api-client": "^1.8.0",
    "@neondatabase/serverless": "^0.9.3",
    "@pulumi/pulumi": "^3.118.0",
    "@upstash/pulumi": "^0.2.0",
    "@upstash/vector": "^1.1.2",
    "drizzle-orm": "^0.31.0",
    "drizzle-zod": "^0.5.1",
    "pg": "^8.11.5",
    "sst": "ion",
    "zod": "^3.23.8"
  },
  "workspaces": [
    "packages/*"
  ]
}