{
  "name": "theaistudybible",
  "description": "The AI Study Bible",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "turbo run build",
    "clean-install": "rimraf pnpm-lock.yaml .sst --glob '**/node_modules' && pnpm cache delete && pnpm install --recursive && sst install",
    "clean": "turbo run clean",
    "cy:open:prod": "sst shell --stage production -- cypress open",
    "cy:open": "sst shell -- cypress open",
    "db:generate": "sst shell --stage production -- drizzle-kit generate",
    "db:migrate:prod": "sst shell --stage production -- drizzle-kit migrate",
    "db:migrate": "sst shell -- drizzle-kit migrate",
    "db:seed:prod": "sst shell --stage production -- tsx ./tools/scripts/src/index.ts db seed",
    "db:seed": "sst shell -- tsx ./tools/scripts/src/index.ts db seed",
    "db:studio:prod": "sst shell --stage production -- drizzle-kit studio",
    "db:studio": "sst shell -- drizzle-kit studio",
    "deploy": "sst deploy",
    "dev": "sst dev",
    "format:check": "biome format .",
    "format": "biome format --write .",
    "lint:fix": "biome lint --write .",
    "lint": "biome lint .",
    "prepare": "husky",
    "prerelease": "pnpm run type-check && pnpm run format && pnpm run lint:fix && pnpm run build && pnpm run test",
    "refresh": "sst refresh",
    "remove": "sst remove",
    "scripts:prod": "sst shell --stage production -- tsx ./tools/scripts/src/index.ts",
    "scripts": "sst shell -- tsx ./tools/scripts/src/index.ts",
    "secret:list": "sst secret list",
    "secret:remove": "sst secret remove",
    "secret:set": "sst secret set",
    "test:e2e:prod": "sst shell --stage production -- cypress run",
    "test:e2e": "sst shell -- cypress run",
    "test": "vitest run",
    "type-check": "turbo run type-check",
    "upgrade": "pnpm update -rL && sst upgrade"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/aws-lambda": "^8.10.149",
    "@types/node": "^22.15.12",
    "@types/web-push": "^3.6.4",
    "cypress": "^14.3.2",
    "cypress-wait-until": "^3.0.2",
    "drizzle-kit": "^0.31.1",
    "husky": "^9.1.7",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.4",
    "turbo": "^2.5.2",
    "typescript": "^5.8.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.3",
    "why-is-node-running": "^3.2.2"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.803.0",
    "@aws-sdk/client-ses": "^3.803.0",
    "@aws-sdk/client-sqs": "^3.803.0",
    "@aws-sdk/s3-request-presigner": "^3.803.0",
    "@langchain/community": "^0.3.42",
    "@langchain/core": "^0.3.53",
    "@libsql/client": "^0.15.4",
    "@upstash/ratelimit": "^2.0.5",
    "@upstash/redis": "^1.34.9",
    "ai": "^4.3.13",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.43.1",
    "drizzle-zod": "0.7.1",
    "langchain": "^0.3.24",
    "sst": "3.14.11",
    "stripe": "^18.1.0",
    "web-push": "^3.6.7",
    "zod": "^3.24.4"
  },
  "packageManager": "pnpm@10.6.1",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "@parcel/watcher",
      "@sentry/cli",
      "core-js",
      "cypress",
      "esbuild",
      "sharp"
    ]
  }
}
