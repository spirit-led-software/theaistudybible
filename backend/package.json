{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "type": "commonjs",
  "scripts": {
    "build": "nest build",
    "build:docker": "nest build && docker build -t septuagint/backend:${npm_package_version} -t septuagint/backend:latest -f Dockerfile .",
    "migrations:generate": "npm run build && typeorm migration:generate -d ./dist/migration.js ./src/app/migrations/migration --pretty",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "devDependencies": {
    "@nestjs/schematics": "^9.1.0",
    "@nestjs/testing": "^9.1.1",
    "@types/cron": "^2.0.1",
    "@types/jest": "^29.4.0",
    "@types/multer": "^1.4.7",
    "@types/node": "~18.7.1",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "jest": "^29.4.1",
    "jest-environment-node": "^29.4.1",
    "prettier": "^2.6.2",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "~5.0.2"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.350.0",
    "@nestjs/bull": "^0.6.3",
    "@nestjs/cache-manager": "^1.0.0",
    "@nestjs/common": "^9.1.1",
    "@nestjs/config": "^2.3.2",
    "@nestjs/core": "^9.1.1",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^9.1.1",
    "@nestjs/schedule": "^2.2.3",
    "@nestjs/typeorm": "^9.0.1",
    "@qdrant/js-client-rest": "^1.2.2",
    "axios": "^1.0.0",
    "axios-retry": "^3.5.0",
    "bull": "^4.10.4",
    "cache-manager": "^5.2.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "fast-xml-parser": "^4.2.4",
    "langchain": "^0.0.91",
    "pg": "^8.11.0",
    "puppeteer": "^19.11.1",
    "redis": "^4.6.7",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "tslib": "^2.3.0",
    "typeorm": "^0.3.16"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
