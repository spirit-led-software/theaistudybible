{
  "name": "@theaistudybible/tasks",
  "description": "The AI Study Bible Tasks",
  "private": true,
  "version": "1.0.0",
  "module": "./src/index.ts",
  "type": "module",
  "scripts": {
    "type-check": "tsc -noEmit",
    "lint": "prettier --check . && eslint .",
    "lint:fix": "prettier --write . && eslint . --fix",
    "format": "prettier --write .",
    "build": "tsc -noEmit",
    "start": "bun run ./src/index.ts",
    "dev": "bun --watch run ./src/index.ts",
    "test": "vitest"
  },
  "devDependencies": {
    "@theaistudybible/eslint-config": "workspace:*",
    "@theaistudybible/prettier-config": "workspace:*",
    "@theaistudybible/tsconfig": "workspace:*",
    "vitest": "^2.0.2"
  },
  "dependencies": {
    "@bull-board/api": "^5.20.5",
    "@bull-board/hono": "^5.20.5",
    "@theaistudybible/ai": "workspace:*",
    "@theaistudybible/core": "workspace:*",
    "bullmq": "^5.8.5",
    "hono": "^4.4.12"
  }
}