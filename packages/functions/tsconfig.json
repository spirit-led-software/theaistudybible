{
  "extends": "@revelationsai/core/tsconfig.json",
  "compilerOptions": {
    "typeRoots": ["./src/types", "../core/src/types"],
    "lib": ["dom", "dom.iterable", "ES2021"],
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "importsNotUsedAsValues": "error",
    "isolatedModules": true,
    "preserveValueImports": true,
    "ignoreDeprecations": "5.0",
    "paths": {
      "@core/configs": ["../core/src/configs"],
      "@core/configs/*": ["../core/src/configs/*"],
      "@core/model": ["../core/src/database/model"],
      "@core/model/*": ["../core/src/database/model/*"],
      "@core/schema": ["../core/src/database/schema"],
      "@core/schema/*": ["../core/src/database/schema/*"],
      "@core/*": ["../core/src/*"],
      "@lib": ["./src/lib"],
      "@lib/*": ["./src/lib/*"],
      "@services": ["./src/services"],
      "@services/*": ["./src/services/*"]
    }
  },
  "include": [
    "**/*.ts",
    "../core/src/langchain/document_loaders/puppeteer-core.ts"
  ],
  "exclude": ["node_modules"]
}
