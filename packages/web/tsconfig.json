{
  "compilerOptions": {
    "typeRoots": ["./src/types", "../core/src/types"],
    "target": "es5",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@core/configs/*": ["../core/src/configs/*"],
      "@core/configs": ["../core/src/configs"],
      "@core/services/*": ["../core/src/services/*"],
      "@core/services": ["../core/src/services"],
      "@chatesv/core/*": ["../core/src/*"],
      "@components/*": ["./src/components/*"],
      "@components": ["./src/components"],
      "@configs/*": ["./src/configs/*"],
      "@configs": ["./src/configs"],
      "@hooks/*": ["./src/hooks/*"],
      "@lib/*": ["./src/lib/*"],
      "@lib": ["./src/lib"],
      "@services/*": ["./src/services/*"],
      "@services": ["./src/services"],
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
