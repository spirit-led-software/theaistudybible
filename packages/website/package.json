	"dependencies": {
		"@revelationsai/core": "*",
		"@tanstack/svelte-query": "^4.32.6",
		"ai": "^2.2.2",
		"autosize": "^6.0.1",
		"moment": "^2.29.4",
		"validator": "^13.11.0"
